{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "devdatafacory"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"LSFileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LSFileServer1'"
		},
		"LsOnPremSQLserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LsOnPremSQLserver'",
			"defaultValue": "Integrated Security=True;Data Source=LAPTOP-2I24GOME;Initial Catalog=db1"
		},
		"LsOnPremSQLserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LsOnPremSQLserver'"
		},
		"datalakegen2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datalakegen2'"
		},
		"AzureSqlDBKV_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLsecret"
		},
		"KVlsLake_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://VNUkv.vault.azure.net/"
		},
		"LSFileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\LAPTOP-2I24GOME\\Azure"
		},
		"LSFileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "venky"
		},
		"LSrestAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/users"
		},
		"LakeLSkv_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "LakeSecret"
		},
		"LsOnPremSQLserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "venky"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "devdatafacory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"datalakegen2_connectionString": {
						"value": "[parameters('datalakegen2_connectionString')]"
					},
					"LSFileServer1_password": {
						"value": "[parameters('LSFileServer1_password')]"
					},
					"LsOnPremSQLserver_connectionString": {
						"value": "[parameters('LsOnPremSQLserver_connectionString')]"
					},
					"LsOnPremSQLserver_password": {
						"value": "[parameters('LsOnPremSQLserver_password')]"
					},
					"KVlsLake_properties_typeProperties_baseUrl": {
						"value": "[parameters('KVlsLake_properties_typeProperties_baseUrl')]"
					},
					"LSrestAPI_properties_typeProperties_url": {
						"value": "[parameters('LSrestAPI_properties_typeProperties_url')]"
					},
					"AzureSqlDBKV_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDBKV_properties_typeProperties_connectionString_secretName')]"
					},
					"LSFileServer1_properties_typeProperties_host": {
						"value": "[parameters('LSFileServer1_properties_typeProperties_host')]"
					},
					"LSFileServer1_properties_typeProperties_userId": {
						"value": "[parameters('LSFileServer1_properties_typeProperties_userId')]"
					},
					"LakeLSkv_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LakeLSkv_properties_typeProperties_connectionString_secretName')]"
					},
					"LsOnPremSQLserver_properties_typeProperties_userName": {
						"value": "[parameters('LsOnPremSQLserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "devdatafacory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'devdatafacory_ArmTemplate_0')]"
			]
		},
		{
			"name": "devdatafacory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'devdatafacory_ArmTemplate_1')]"
			]
		}
	]
}